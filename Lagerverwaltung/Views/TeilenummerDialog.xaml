<Window x:Class="Lagerverwaltung.Views.TeileNummerDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Teilenummer eingeben" Height="150" Width="300"
        WindowStartupLocation="CenterOwner">
    <StackPanel Margin="10">
        <TextBlock Text="Bitte Teilenummer eingeben:" Margin="0,0,0,5"/>
        <TextBox x:Name="txtTeilenummer" Margin="0,0,0,10"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
            <Button Content="OK" Width="75" Margin="0,0,5,0" Click="Ok_Click"/>
            <Button Content="Abbrechen" Width="75" Click="Cancel_Click"/>
        </StackPanel>
    </StackPanel>
</Window>
